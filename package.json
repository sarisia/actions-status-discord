    "release": "npm ci && npm run build && npm prune --production",
    "version": "npm run release && git add .",
    "deps": "ncu --doctor -u"
  },
  "author": "Sarisia",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "jest": "^27.5.1",
    "npm-check-updates": "^12.5.5",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@actions/github": "^5.0.1",
    "@types/node": "^18.6.4",
    "axios": "^0.26.1"
  }
}
