{
  "name": "actions-status-discord",
  "version": "0.0.0",
  "description": "Post Github Actions CI status to Discord",
  "main": "lib/main.js",
  "scripts": {
    "test": "jest --verbose --ci --detectOpenHandles && npm run build",
